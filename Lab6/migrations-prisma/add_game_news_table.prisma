model game_news {
  id           Int      @id @default(autoincrement())
  game_id      Int
  title        String   @db.VarChar(200)
  content      String
  published_at DateTime @default(now()) @db.Timestamp(6)
  games        games    @relation(fields: [game_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model games {
  game_news game_news[]
}